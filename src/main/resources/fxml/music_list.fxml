<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox id="RootBox" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="us.guihouse.projector.forms.controllers.MusicListController">
   <children>
      <ToolBar prefHeight="40.0" prefWidth="200.0">
         <items>
            <HBox alignment="TOP_RIGHT" prefHeight="27.0" prefWidth="581.0">
               <children>
                  <ImageView fitHeight="19.0" fitWidth="17.0" preserveRatio="true" HBox.hgrow="NEVER">
                     <image>
                        <Image url="@../icons/icon-ios7-search-strong-128.png" />
                     </image>
                     <HBox.margin>
                        <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
                     </HBox.margin>
                  </ImageView>
                  <TextField fx:id="searchText" maxWidth="1.7976931348623157E308" prefHeight="27.0" prefWidth="407.0" promptText="Pesquisar (Nome, Artista ou Trecho da letra)" style="-fx-background-radius: 10;" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
         </items>
      </ToolBar>
      <TableView fx:id="resultsTable" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
        <columns>
          <TableColumn prefWidth="75.0" text="C1" />
          <TableColumn prefWidth="75.0" text="C2" />
        </columns>
      </TableView>
      <ToolBar prefHeight="40.0" prefWidth="200.0">
        <items>
            <Button mnemonicParsing="false" onAction="#onManualType" text="Digitar Letra" />
          <Button mnemonicParsing="false" onAction="#onWebImport" text="Importar de um Site" />
        </items>
      </ToolBar>
   </children>
</VBox>
