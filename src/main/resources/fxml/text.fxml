<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<VBox id="RootBox" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="us.guihouse.projector.forms.controllers.TextController">
   <children>
      <HBox maxWidth="1.7976931348623157E308" VBox.vgrow="NEVER">
         <children>
            <Button fx:id="beginProjectionButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onBeginProjection" text="Projetar na Tela" HBox.hgrow="ALWAYS" />
            <Button fx:id="endProjectionButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onEndProjection" text="Remover da Tela (ESC)" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <Label text="Digite o Texto:">
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </Label>
      <TextField fx:id="projectionText">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </TextField>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
